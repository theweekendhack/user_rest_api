{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "username": {
      "type": "string",
      "minLength": 6,
      "maxLength": 30
    },
    "password": {
      "type": "string",
      "minLength": 7,
      "maxLength": 15
    },
    "first_name": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "last_name": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "location": {
      "type": "string"
    }
  },
  "additionalProperties": false,
  "required": [
    "username",
    "first_name",
    "last_name",
    "password",
    "location"
  ]
}
